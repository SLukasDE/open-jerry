	<object id="x" implementation="jerry/builtin/procedure/sleep">
		<parameter key="sleep-ms" value="1000"/>
	</object>
	
	<object id="y" implementation="jerry/builtin/procedure/sleep">
		<parameter key="sleep-ms" value="1000"/>
	</object>
	
	<daemon implementation="jerry/builtin/daemon/procedures">
		<parameter key="procedure-id" value="x"/>
		<parameter key="procedure-id" value="y"/>
	</daemon>
